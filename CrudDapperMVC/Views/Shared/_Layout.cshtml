<!doctype html>
<html lang="en" class="h-100">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
    <meta name="generator" content="Jekyll v4.1.1">
    <title>Cadastro de Cliente | Dapper - MVC5</title>
    <link rel="canonical" href="https://getbootstrap.com/docs/4.5/examples/album/">
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/toastrCss")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="d-flex flex-column h-100">
    <header>
        <div class="navbar navbar-dark bg-dark shadow-sm">
            <div class="container d-flex justify-content-between">
                <a href="#" class="navbar-brand d-flex align-items-center">
                    <strong>Cadastro de Cliente</strong>
                </a>
            </div>
        </div>
    </header>
    <main role="main">
        <div class="album py-5">
            <div class="container">
                @RenderBody()
            </div>
        </div>
    </main>
    <footer class="footer mt-auto py-3">
        <div class="container">
            <span class="text-muted">Cadastro de Cliente.</span>
        </div>
    </footer>

    @Html.Partial("~/Views/Shared/PartialView/_ShowToastr.cshtml")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/bundles/toastrJs")
    @Scripts.Render("~/bundles/dataTablesJs")
    @RenderSection("scripts", required: false)
</body>
</html>